{
	"name": "root",
	"version": "1.2.2",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-lerna-changelog"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"commitizen": "^4.0.4",
		"cz-lerna-changelog": "^2.0.2",
		"eslint": "^7.0.0",
		"eslint-plugin-react": "^7.19.0",
		"hooks": "^0.3.2",
		"husky": "^4.2.5",
		"lerna": "^3.20.2",
		"lint-staged": "^10.1.3",
		"standard": "^14.3.3"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"yarn lint:code",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"react": "^16.4.0",
		"typescript": "^3.7.5"
	}
}
